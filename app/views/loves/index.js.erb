<%= cache ['v1', @list, 'lovers'] do %>
  $('#list_side_info').html("<%= escape_javascript(render(partial: 'loves/user_list', locals: { users: @users })) %>").hide().fadeIn();
  $('#list_nav .comments').parent().removeClass('active');
  $('#list_nav .lovers').parent().addClass('active');
  window.Echo.init();
<% end %>
