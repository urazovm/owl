$('#love_button .love').hide();
$('#love_button .ignore').hide().fadeIn();
var count = $(".total_lovers_<%= @list.id %>");
count.html(count.html().replace(/\d+/g, function(x) { return Number(x) + 1; }));
<% if request.params['controller'] == 'loves' %>
$('#loves_user_list ul').append("<%= escape_javascript(render(partial: 'users/item', locals: { user: current_user })) %>");
$('#user_' + '<%= current_user.id %>' + ' .follow').remove();
<% end %>
