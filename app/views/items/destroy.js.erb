var item = $("#item_<%= @item.id %>");
item.fadeOut(function() { $(this).remove(); });
$('#item_new_form .btn').removeClass('disabled');
$('#item_new_form').fadeIn();
<% if @item.list.completed %>
  $('#completed_button').removeClass('hidden');
<% else %>
  $('#completed_button').addClass('hidden');
<% end %>
